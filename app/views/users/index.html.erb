<%= form_for @user , :html => {:id => "level_forms"}  do |f| %>
	<div class="form-group">
		<%= f.label :useremail, "User Email :" %>
		<%= f.text_field :email , :class => "form-control" , :placeholder => "useremail" %>
	</div>
	<div class="form-group">
		<%= f.label :password, "User Password :" %>
		<%= f.password_field :password , :class => "form-control" , :placeholder => "password" %>
	</div>
	<%= f.submit :Create , :class => "btn btn-primary" %>
<% end %>